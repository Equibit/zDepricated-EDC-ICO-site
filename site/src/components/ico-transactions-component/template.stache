{{#if data.length}}
  <div class="row">
    <div class="col-md-12 text-center">

      <div class="h2">Your Transactions</div>

      <div class="panel">
        <div class="panel-body">

          <div class="table-responsive">
            <table class="table table-striped">
              <thead>
              <tr>
                <th>EQB Purchased</th>
                <th>Funding Level</th>
                <th>Paid</th>
                <th>Time and Date</th>
                <th>Confirmed</th>
              </tr>
              </thead>
              <tbody>
              {{#if loaded}}
                {{#each data}}
                  <tr>
                    <td>{{numberEQB}}</td>
                    <td>{{fundingLevel}}</td>
                    <td>{{#if paidUSD}}${{paidUSD}} USD{{/if}}{{#if paidBTC}}{{paidBTC}} BTC{{/if}}</td>
                    <td>{{timestampDetailed timeDate}}</td>
                    <td>{{#if completed}}<span class="green-text fa fa-check"></span>{{else}}<span class="red-text fa fa-times"></span>{{/if}}</td>
                  </tr>
                {{/each}}
              {{else}}
                <tr>
                  <td colspan="4" class="text-center">
                    <br>
                    <span class="fa fa-refresh fa-3x fa-spin"></span>
                    <br><br>
                  </td>
                </tr>
              {{/if}}
              </tbody>
            </table>
          </div>

        </div>
      </div>
    </div>
  </div>

  <br>
{{/if}}